<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_im_seller_form" model="ir.ui.view">
            <field name="name">im.seller.form</field>
            <field name="model">im.seller</field>
            <field name="arch" type="xml">
                <form string="seller of Personal Appreciation" version="7.0">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only" string="Name"/>
                            <h1>
                                <field name="name" class="oe_inline" placeholder="seller"/>
                            </h1>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_im_seller_tree" model="ir.ui.view">
            <field name="name">im.seller.tree</field>
            <field name="model">im.seller</field>
            <field name="arch" type="xml">
                <tree string="seller">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="view_im_seller_search" model="ir.ui.view">
            <field name="name">im.seller.search</field>
            <field name="model">im.seller</field>
            <field name="arch" type="xml">
                <search string="seller">
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_open_im_seller_view">
            <field name="name">Seller</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">im.seller</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_im_seller_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create a new seller.
                </p>
            </field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_im_seller_view_form">
            <field name="act_window_id" ref="act_open_im_seller_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_im_seller_form"/>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_im_seller_view_tree">
            <field name="act_window_id" ref="act_open_im_seller_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_im_seller_tree"/>
        </record>
    </data>
</openerp>