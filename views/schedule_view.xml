<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_im_schedule_form" model="ir.ui.view">
            <field name="name">im.schedule.form</field>
            <field name="model">im.schedule</field>
            <field name="arch" type="xml">
                <form string="schedule of Personal Appreciation" version="7.0">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only" string="Name"/>
                            <h1>
                                <field name="name" class="oe_inline" placeholder="schedule"/>
                            </h1>
                        </div>
                        <group>
                            <field name="customer_id"/>
                            <field name="seller_id"/>
                            <field name="latitude"/>
                            <field name="longitude"/>
                            <field name="date"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_im_schedule_tree" model="ir.ui.view">
            <field name="name">im.schedule.tree</field>
            <field name="model">im.schedule</field>
            <field name="arch" type="xml">
                <tree string="schedule">
                    <field name="customer_id"/>
                    <field name="seller_id"/>
                    <field name="latitude"/>
                    <field name="longitude"/>
                    <field name="date"/>
                </tree>
            </field>
        </record>
        <record id="view_im_schedule_search" model="ir.ui.view">
            <field name="name">im.schedule.search</field>
            <field name="model">im.schedule</field>
            <field name="arch" type="xml">
                <search string="schedule">
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record id="view_im_schedule_map0" model="ir.ui.view">
            <field name="name">map</field>
            <field name="model">im.schedule</field>
            <field eval="16" name="priority"/>
            <field name="arch" type="xml">
                <geoengine>
                    <field name="name"/>
                </geoengine>
            </field>
        </record>
        <!--<record id="ir_ui_view_retailmachineview0" model="ir.ui.view">-->
            <!--<field name="name">Retail machine view</field>-->
            <!--<field name="arch" type="xml">-->
                <!--&lt;!&ndash;<geoengine version="7.0">&ndash;&gt;-->
                    <!--&lt;!&ndash;<field namsde="the_poing" select="1"/>&ndash;&gt;-->
                <!--&lt;!&ndash;</geoengine>&ndash;&gt;-->
            <!--</field>-->
            <!--<field eval="16" name="priority"/>-->
            <!--<field name="model">im.sdfschedule</field>-->
        <!--</record>-->

        <record model="ir.actions.act_window" id="act_open_im_schedule_view">
            <field name="name">Schedule</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">im.schedule</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,geoengine</field>
            <field name="view_id" ref="view_im_schedule_tree"/>
            <field name="search_view_id" ref="view_im_schedule_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create a new schedule.
                </p>
            </field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_im_schedule_view_form">
            <field name="act_window_id" ref="act_open_im_schedule_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_im_schedule_form"/>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_im_schedule_view_tree">
            <field name="act_window_id" ref="act_open_im_schedule_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_im_schedule_tree"/>
        </record>
    </data>
</openerp>